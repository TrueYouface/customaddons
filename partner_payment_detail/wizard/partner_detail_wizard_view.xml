        <record model="ir.ui.view" id="customer_excel_form">
            <field name="name">customer.excel.form</field>
            <field name="model">customer.excel</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Customer Excel">
                    <group>
	                    <field name="file" filename="name" nolabel="0" readonly="1"/>
	                    <field name="name" invisible='1'/>
                    </group>
                    <footer>
                    	<button name="download_xlsx" type="object" string="Download" class="oe_highlight"/>
                        <button string="Cancel" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>